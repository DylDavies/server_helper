<h1>User List</h1>

<div class="row">
{{#each users}}
    <div class="col-md-4">
        <h2>{{this.username}}</h2>
        <p>
            {{this.bio}}
        </p>
        <p>
            <a class="btn btn-default" href="/accounts/{{this.username}}" role="button">View details &raquo;</a>
        </p>
    </div>
{{/each}}
</div>